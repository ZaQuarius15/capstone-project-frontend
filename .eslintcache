          }\n        })\n    }\n\n    render() {\n        return (\n            <div class='App'>\n                {/* <NavBar /> */}\n                {this.state.error ? <div><br/><br/><h4 style={{color: 'red'}}>{this.state.error}</h4></div> : null}\n                <Grid textAlign='center' style={{ height: '100vh' }} verticalAlign='middle'>\n                    <Grid.Column style={{ maxWidth: 450 }}>\n                        <Header as='h2' color='blue' style={{backgroundColor: 'white'}} textAlign='center'>\n                            Log-in to your account\n                        </Header>\n                        <Form size='large' onSubmit={this.handleSubmit}>\n                            <Segment stacked>\n                            <Form.Input onChange={this.handleInputChange} icon='user' name='username' value={this.state.username} iconPosition='left' placeholder='Username' />\n                            <Form.Input\n                                onChange={this.handleInputChange}\n                                icon='lock'\n                                iconPosition='left'\n                                placeholder='Password'\n                                name='password'\n                                value={this.state.password}\n                                type='password'\n                            />\n\n                            <Button color='blue' fluid size='large'>\n                                Login\n                            </Button>\n                            </Segment>\n                        </Form>\n                        <Message>\n                            New to us? <a href='#'>Sign Up</a>\n                        </Message>\n                    </Grid.Column>\n                </Grid>\n                {/* <br/><br/><h3>Sign In</h3>\n                <form onSubmit={this.handleSubmit}>\n                    <input name={'username'} onChange={this.handleInputChange} value={this.state.username} placeholder='Enter username...'/>\n                    <input type='password' name={'password'} onChange={this.handleInputChange} value={this.state.password} placeholder='Enter password...'/>\n                    <input type='submit' value='login'/>\n                </form> */}\n            </div>\n        )\n    }\n}\n\nconst mapDispatchToProps = {\n    loginSuccess\n}\n\nexport default connect(null, mapDispatchToProps)(Login)\n","/Users/JohnZacharyCarlson/mod-5-capstone/capstone-project-frontend/src/actions/auth.js",[],"/Users/JohnZacharyCarlson/mod-5-capstone/capstone-project-frontend/src/reducers/auth.js",[],"/Users/JohnZacharyCarlson/mod-5-capstone/capstone-project-frontend/src/reducers/libraryBooks.js",[],["140","141"],"/Users/JohnZacharyCarlson/mod-5-capstone/capstone-project-frontend/src/reducers/wishedBooks.js",[],"/Users/JohnZacharyCarlson/mod-5-capstone/capstone-project-frontend/src/components/Profile.js",[],"/Users/JohnZacharyCarlson/mod-5-capstone/capstone-project-frontend/src/components/UserCard.js",[],"/Users/JohnZacharyCarlson/mod-5-capstone/capstone-project-frontend/src/components/LibraryBooks.js",[],"/Users/JohnZacharyCarlson/mod-5-capstone/capstone-project-frontend/src/components/LibraryBookCard.js",[],"/Users/JohnZacharyCarlson/mod-5-capstone/capstone-project-frontend/src/components/WishedBooks.js",[],"/Users/JohnZacharyCarlson/mod-5-capstone/capstone-project-frontend/src/components/WishedBookCard.js",[],"/Users/JohnZacharyCarlson/mod-5-capstone/capstone-project-frontend/src/actions/index.js",[],"/Users/JohnZacharyCarlson/mod-5-capstone/capstone-project-frontend/src/components/AddLibraryBookList.js",[],"/Users/JohnZacharyCarlson/mod-5-capstone/capstone-project-frontend/src/components/AddLibraryBookCard.js",[],"/Users/JohnZacharyCarlson/mod-5-capstone/capstone-project-frontend/src/components/AddWishedBookList.js",[],"/Users/JohnZacharyCarlson/mod-5-capstone/capstone-project-frontend/src/components/AddWishedBookCard.js",[],"/Users/JohnZacharyCarlson/mod-5-capstone/ca
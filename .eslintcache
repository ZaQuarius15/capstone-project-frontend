)\n    }\n\n    reservedBooks = () => {\n        return this.props.reservedBooks.filter(b => b.user_id === this.props.auth.id && b.delivered === false)\n    }\n\n    reservedFromMyLibrary = () => {\n        return this.props.allLibraryBooks.filter(book => book[1].id === this.props.auth.id && this.props.reservedBooks.find(b => b.user_lib_book_id === book[2] && b.delivered === false))\n    }\n\n    render() {\n        window.scrollTo(0, 0)\n        return (\n            <div className='App'>\n                <NavBar />\n                <br/><br/><Header as='h2' icon style={{color: 'white'}} textAlign=\"center\">\n                    <Icon name='book' circular />\n                    <Header.Content>Your Reserved Books</Header.Content>\n                </Header>\n                {this.reservedBooks().length !== 0 ?\n                <Grid>\n                    <Grid.Column width='1'></Grid.Column>\n                    <Grid.Column width='14'>\n                        <div>\n                            <br/><br/><div className='ui seven centered cards'>\n                                {this.props.allLibraryBooks.map((book, i) => {\n                                    if (this.reservedBooks().find(b => b.user_lib_book_id === book[2])){\n                                        return <DashboardLibraryBookCard key={i} book={book[0]} user={book[1]} userBookId={book[2]}/>\n                                    }\n                                })}\n                            </div><br/><br/><br/><br/>\n                        </div>\n                    </Grid.Column>\n                </Grid>\n                :\n                <div>\n                    <br/><br/><Header as='h3' style={{color: 'white'}} textAlign=\"center\">\n                        You do not have any Reserved Books at the moment\n                    </Header><br/><br/>\n                    <Header as='h3' style={{color: 'white'}} textAlign=\"center\">\n                        Search for Books to Reserve\n                    </Header><br/>\n                    <Grid>\n                        <Grid.Column textAlign=\"center\">\n                            <Button as={ Link } exact='true' to={`/books`} color='blue'>All Books</Button>\n                        </Grid.Column>\n                    </Grid><br/><br/>\n                </div>\n                }\n                <br/><br/><Header as='h2' icon style={{color: 'white'}} textAlign=\"center\">\n                    <Icon name='book' circular />\n                    <Header.Content>Books Reserved from Your Library</Header.Content>\n                </Header>\n                {this.reservedFromMyLibrary().length !== 0 ?\n                    <Grid>\n                        <Grid.Column width='1'></Grid.Column>\n                        <Grid.Column width='14'>\n                            <div>\n                                <br/><br/><div className='ui seven centered cards'>\n                                    {this.reservedFromMyLibrary().map((book, i) => {\n                                        return <LibraryBookCard key={i} book={book[0]} user={book[1]} userBookId={book[2]}/>\n                                    })}\n                                </div><br/><br/><br/><br/>\n                            </div>\n                        </Grid.Column>\n                    </Grid>\n                    :\n                    <div>\n                    <br/><br/><Header as='h3' style={{color: 'white'}} textAlign=\"center\">\n                        You do not have any Reserved Books at the moment\n                    </Header><br/><br/>\n                    <Header as='h3' style={{color: 'white'}} textAlign=\"center\">\n                        Search for Books to Reserve\n                    </Header><br/>\n                    <Grid>\n                        <Grid.Column textAlign=\"center\">\n                            <Button color='blue'>All Books</Button>\n                        </Grid.Column>\n                    </Grid><br/><br/>\n                </div>\n                }\n                <Footer/>\n            </div>\n        )\n    }\n}\n\nconst mapStateToProps = state => {\n    return {\n        allLibraryBooks: state.allLibraryBooks,\n        reservedBooks: state.reservedBooks,\n        auth: state.auth\n    }\n}\n\nexport default connect(mapStateToProps, { showBook })(ReservedBooks)\n","/Users/JohnZacharyCarlson/REACT_APPS/mod-5-capstone/capstone-project-frontend/src/components/ReservedBookShowPage.js",[],"/Users/JohnZacharyCarlson/REACT_APPS/mod-5-capstone/capstone-project-frontend/src/reducers/showBook.js",[],"/Users/JohnZacharyCarlson/REACT_APPS/mod-5-capstone/capstone-project-frontend/src/reducers/allLibraryBooks.js",[],"/Users/JohnZacharyCarlson/REACT_APPS/mod-5-capstone/capstone-project-frontend/src/reducers/showUser.js",[],"/Users/JohnZacharyCarlson/REACT_APPS/mod-5-capstone/capstone-project-frontend/src/reducers/showReservedBook.js",[],"/Users/JohnZacharyCarlson/REACT_APPS/mod-5-capstone/capstone-project-frontend/src/reducers/allWishedBooks.js",[],"/Users/JohnZacharyCarlson/REACT_APPS/mod-5-capstone/capstone-project-frontend/src/reducers/allComments.js",[],"/Users/JohnZacharyCarlson/REACT_APPS/mod-5-capstone/capstone-project-frontend/src/reducers/allMessages.js",[],"/Users/JohnZacharyCarlson/REACT_APPS/mod-5-capstone/capstone-project-frontend/src/reducers/reservedBooks.js",[],"/Users/JohnZacharyCarlson/REACT_APPS/mod-5-capstone/capstone-project-frontend/src/actions/auth.js",[],"/Users/JohnZacharyCarlson/REACT_APPS/mod-5-capstone/capstone-project-frontend/src/actions/index.js",[],"/Users/JohnZacharyCarlson/REACT_APPS/mod-5-capstone/capstone-project-frontend/src/reducers/auth.js",[],"/Users/JohnZacharyCarlson/REACT_APPS/mod-5-capstone/capstone-project-frontend/src/components/NavBar.js",[],"/Users/JohnZacharyCarlson/REACT_APPS/mod-5-capstone/capstone-project-frontend/src/components/LibraryBooks.js",[],"/Users/JohnZacharyCarlson/REACT_APPS/mod-5-capstone/capsto
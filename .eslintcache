ite'}} textAlign='center'>\n                                <Icon name='user' circular />\n                                <Header.Content>\n                                    MyBrarians\n                                </Header.Content>\n                            </Header>\n                        </Grid.Row>\n                        <Grid.Row></Grid.Row>\n                        </Grid>\n                            {this.fellowMyBrarians().map(u =>   \n                            <Grid textAlign='center'>   \n                                <Grid.Row> \n                                    <Grid.Column width='1'></Grid.Column>\n                                    <Grid.Column width='4' verticalAlign='middle'>\n                                        <Grid.Row>\n                                        <Popup\n                                            content={u.bio}\n                                            key={u.username}\n                                            header={u.username}\n                                            trigger={<Image src={u.prof_pic_url} avatar size='large'/>}\n                                        />\n                                        </Grid.Row><br/>\n                                        <Grid.Row>\n                                            <Header as='h3' style={{color: 'white'}}>{u.username}</Header>\n                                        </Grid.Row><br/>\n                                        <Grid.Row>\n                                            <Button color='purple'><Icon name='heart'/>Friend</Button>\n                                        </Grid.Row>\n                                    </Grid.Column>\n                                    <Grid.Column width='2'></Grid.Column>\n                                        {this.myBrarianLibraryBooks(u.id).length === 0 ?\n                                            <Grid.Column width='8' verticalAlign='middle'>\n                                             